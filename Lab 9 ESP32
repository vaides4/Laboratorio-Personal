#include <Arduino.h>

//potenciometros
#define pot1 36
#define pot2 39

//variables donde se guardaran los valores de los poten
int valorPot1, valorPot2;

void setup() {
  Serial1.begin(115200);
}

void loop() {
  //lectura de potenciometros
  valorPot1 = analogReadMilliVolts(pot1)/10.0;
  valorPot2 = analogReadMilliVolts(pot2)/10.0;
  Serial1.print(valorPot1);
  Serial1.print(" , ");
  Serial1.println(valorPot2);
}
